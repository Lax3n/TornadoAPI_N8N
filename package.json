{
  "name": "n8n-nodes-tornado-api",
  "version": "1.0.0",
  "description": "n8n node for Tornado API - Download YouTube videos & Spotify podcasts",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "tornado",
    "video",
    "downloader",
    "youtube",
    "spotify",
    "podcast",
    "mp4",
    "mp3"
  ],
  "license": "MIT",
  "homepage": "https://tornado.velys.software",
  "author": {
    "name": "Velys Software",
    "email": "support@velys.software"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Lax3n/TornadoAPI_N8N.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint \"nodes/**/*.ts\" \"credentials/**/*.ts\"",
    "lintfix": "eslint \"nodes/**/*.ts\" \"credentials/**/*.ts\" --fix",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/TornadoApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/TornadoApi/TornadoApi.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.56.0",
    "gulp": "^5.0.1",
    "n8n-workflow": "^1.17.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
